        {
          "alias": false,
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              "& when (@overlay)": {
                ".mbr-overlay": {
                  "background": "@overlayColor",
                  "opacity": "@overlayOpacity"
                }
              }
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when not (@blockDirection)": {
              ".title-block": {
                "margin": "auto",
                "width": "100%"
              },
              ".title-wrap": {
                "@media (max-width: 992px)": {
                  "margin-bottom": "2rem"
                }
              },
              ".mbr-form": {
                "justify-content": "center"
              }
            },
            ".form-block": {
              ".mbr-overlay": {
                "border-radius": "15px",
                "opacity": "@formBgOpacity",
                "background": "@formBgColor !important"
              },
              "& when (@transparentForm)": {
                "@media (min-width: 992px)": {
                  "padding-left": "2rem",
                  "padding-right": "2rem"
                }
              },
              "input, textarea, select": {
                "border-radius": "10px !important",
                "color": "@labelsColor !important",
                "border": "1px solid @borderColor",
                "&:hover": {
                  "border": "1px solid @primaryColor"
                },
                "background": "@inputColor"
              },
              "input::-webkit-input-placeholder": {
                "color": "fade(@labelsColor, 50%)"
              },
              "input::-moz-placeholder": {
                "color": "fade(@labelsColor, 50%)"
              },
              "textarea::-webkit-input-placeholder": {
                "color": "fade(@labelsColor, 50%)"
              },
              "textarea::-moz-placeholder": {
                "color": "fade(@labelsColor, 50%)"
              },
              ".mbr-section-btn": {
                "margin": "0.5rem"
              }
            },
            ".mbr-section-btn": {
              "margin": "0"
            },
            ".mbr-section-btn a:first-child": {
              "margin-left": "0"
            },
            ".title-block": {
              "padding-left": "0",
              "padding-right": "0"
            },
            ".btn": {
              "padding": "10px 50px"
            },
            "form .fieldset": {
              "@media (max-width: 767px)": {
                "display": "block"
              }
            },
            "textarea": {
              "min-height": "200px"
            }
          },
          "_name": "header4",
          "_customHTML": "<section class=\"header4\" group=\"Headers\" mbr-class=\"{'mbr-fullscreen': fullScreen}\" plugins=\"amp-main, amp-form, amp-mustache\">\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Row/Column\" name=\"blockDirection\">\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" checked condition=\"!blockDirection\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"15\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"15\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Form Width\" name=\"columnWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"!blockDirection\" required>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\">\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\">\n        <header>Align</header>\n        <select title=\"Title\" name=\"alignTitle\" condition=\"(showTitle || showSubtitle || showText || showButtons) && blockDirection\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected>Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <select title=\"Form\" name=\"alignForm\" condition=\"(showTitle || showSubtitle || showText || showButtons) && blockDirection\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected>Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <select title=\"Text\" name=\"alignContent\" condition=\"(showTitle || showSubtitle || showText || showButtons) && !blockDirection\">\n            <option value=\"1\" selected>Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <select title=\"Text Form\" name=\"alignText\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected>Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <header>Form</header>\n        <input type=\"checkbox\" title=\"Show Form Title\" name=\"showFormTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Form Subtitle\" name=\"showFormSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Show Form Text\" name=\"showFormText\">\n\n        <input type=\"checkbox\" title=\"Color Form\" name=\"transparentForm\" checked>\n        <input type=\"color\" title=\"Form Bg Color\" name=\"formBgColor\" value=\"#232323\" condition=\"transparentForm\" required>\n        <input type=\"range\" inline title=\"Form Bg Opacity\" name=\"formBgOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"transparentForm\">\n        <input type=\"color\" title=\"Fields Color\" name=\"inputColor\" value=\"#232323\">\n        <input type=\"color\" title=\"Border Color\" name=\"borderColor\" value=\"#292845\">\n        <input type=\"color\" title=\"Form Text Color\" name=\"labelsColor\" value=\"#ffffff\">\n        <header condition=\"blockDirection\">Content</header>\n        <input type=\"range\" inline title=\"Content Width\" name=\"contentWidth\" min=\"1\" max=\"6\" step=\"1\" value=\"3\" condition=\"blockDirection\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" required>\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/dscf9403-2000x1193.jpg\" selected>\n            <input type=\"color\" title=\"Background Color\" value=\"#cccccc\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n        'container-fluid': fullWidth}\">\n        <div class=\"mbr-row mbr-black mbr-jc-c main-row\" mbr-class=\"{'mbr-row-reverse': reverseContent}\">\n            <div class=\"title-wrap mbr-col-md-12 mbr-col-sm-12\" mbr-class=\"{'mbr-col-lg-3':columnWidth==1 && !blockDirection,\n                'mbr-col-lg-4':columnWidth==2 && !blockDirection,\n                'mbr-col-lg-6':columnWidth==3 && !blockDirection,\n                'mbr-col-lg-8':columnWidth==4 && !blockDirection,\n                'mbr-col-lg-9':columnWidth==5 && !blockDirection,\n                'mbr-col-lg-12':blockDirection,\n                'align-center':(alignTitle==2 && blockDirection) || (alignContent==2 && !blockDirection),\n                'align-right':(alignTitle==3 && blockDirection) || (alignContent==3 && !blockDirection),\n                'mbr-pb-4': blockDirection,\n                'mbr-flex': !blockDirection}\" mbr-if=\"showTitle || showSubtitle || showText || showButtons\">\n                <div class=\"title-block mbr-col-sm-12 mbr-col-md-12\">\n                    <h1 class=\"mbr-section-title mbr-fonts-style mbr-bold mbr-white\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">COLLABORATE<br>WITH US</h1>\n                    <h2 class=\"mbr-section-subtitle mbr-fonts-style mbr-white\" mbr-class=\"{'mbr-pt-3': showTitle}\" mbr-theme-style=\"display-2\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">\n                        Enter your subtitle here</h2>\n                    <p class=\"mbr-text mbr-fonts-style mbr-pt-2 mbr-white\" mbr-class=\"{'mbr-pt-2': showTitle || showSubtitle}\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">Click on the text in the block to select it and\n                        enter the text you want to show.</p>\n                    <div mbr-buttons class=\"mbr-section-btn mbr-pt-4\" mbr-class=\"{'mbr-pt-4': showTitle || showSubtitle || showText}\" mbr-theme-style=\"display-7\" data-toolbar=\"-mbrBtnMove\" mbr-if=\"showButtons\">\n                        <a class=\"btn-primary btn\">Learn How</a>\n                    </div>\n                </div>\n            </div>\n            <div class=\"form-wrap mbr-col-md-12 mbr-col-sm-12\" mbr-class=\"{'mbr-col-lg-3':columnWidth==5 && !blockDirection,\n                'mbr-col-lg-4':columnWidth==4 && !blockDirection,\n                'mbr-col-lg-6':columnWidth==3 && !blockDirection,\n                'mbr-col-lg-8':columnWidth==2 && !blockDirection,\n                'mbr-col-lg-9':columnWidth==1 && !blockDirection,\n                'mbr-col-lg-12':blockDirection,\n                'mbr-flex':!blockDirection}\">\n                <div class=\"form-block mbr-col-md-12 mbr-col-sm-12\" mbr-class=\"{'mbr-col-lg-3':contentWidth==1 && blockDirection,\n                    'mbr-col-lg-4':contentWidth==2 && blockDirection,\n                    'mbr-col-lg-6':contentWidth==3 && blockDirection,\n                    'mbr-col-lg-8':contentWidth==4 && blockDirection,\n                    'mbr-col-lg-9':contentWidth==5 && blockDirection,\n                    'mbr-col-lg-12': !blockDirection,\n                    'mbr-mr-auto':alignForm==1,\n                    'mbr-m-auto':alignForm==2,\n                    'mbr-ml-auto':alignForm==3,\n                    'align-center': alignText == 2,\n                    'align-right': alignText == 3}\">\n                    <div class=\"mbr-col-md-12\">\n                        <h3 mbr-theme-style=\"display-2\" class=\"mbr-fonts-style form-title mbr-fonts-style mbr-white mbr-pb-2\">Apply Now</h3>\n                    </div>\n                    <div class=\"mbr-col-md-12\">\n                        <h4 mbr-theme-style=\"display-7\" class=\"mbr-fonts-style mbr-fonts-style form-subtitle mbr-white mbr-pb-4\">Join the 75+ connected schools that are currently signed up to participate in our eSports Tournaments.<div><br></div><div>You can join with a small or large group of Students and start empowering them to co-develop an interconnected learning journey focused on eSports.&nbsp;</div></h4>\n                    </div>\n                    <div class=\"mbr-overlay\" mbr-if=\"transparentForm\"></div>\n                    <div mbr-form>\n                      <form action=\"esportseduacademy@gmail.com\" name=\"8b Form\" method=\"post\" class=\"mbr-form\">\n                          <div class=\"mbr-row\">\n                              <div submit-success class=\"mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n                                  <template data-form-alert type=\"amp-mustache\">We'll get back to you ASAP!</template>\n                              </div>\n                              <div submit-error class=\"mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n                                  <template data-form-alert type=\"amp-mustache\">We'll get back to you ASAP!</template>\n                              </div>\n                          </div>\n                          <div class=\"dragArea mbr-row\">\n\n                              <div data-for=\"form[data][0][1]\" class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-6 field\">\n                                  <input type=\"hidden\" name=\"form[data][0][0]\" value=\"Name\">\n                                  <input class=\"field-input\" type=\"text\" name=\"form[data][0][1]\" placeholder=\"Name\" data-form-field=\"Name\" mbr-theme-style=\"display-7\" value>\n                              </div>\n                              <div data-for=\"form[data][1][1]\" class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-6 field\">\n                                  <input type=\"hidden\" name=\"form[data][1][0]\" value=\"Last Name\">\n                                  <input class=\"field-input\" type=\"text\" name=\"form[data][1][1]\" placeholder=\"School Name\" data-form-field=\"Last Name\" mbr-theme-style=\"display-7\" value>\n                              </div>\n                              <div data-for=\"form[data][2][1]\" class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-6 field\">\n                                  <input type=\"hidden\" name=\"form[data][2][0]\" value=\"E-mail\">\n                                  <input class=\"field-input\" type=\"email\" name=\"form[data][2][1]\" placeholder=\"Education Email\" data-form-field=\"E-mail\" mbr-theme-style=\"display-7\" value>\n                              </div>\n                              <div data-for=\"form[data][3][1]\" class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-6 field\">\n                                  <input type=\"hidden\" name=\"form[data][3][0]\" value=\"Phone\">\n                                  <input class=\"field-input\" type=\"tel\" name=\"form[data][3][1]\" placeholder=\"Country\" data-form-field=\"Phone\" mbr-theme-style=\"display-7\" value>\n                              </div>\n                              <div data-for=\"form[data][4][1]\" class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-12 field\">\n                                  <input type=\"hidden\" name=\"form[data][4][0]\" value=\"Message\">\n                                  <textarea class=\"field-input\" name=\"form[data][4][1]\" placeholder=\"Message\" data-form-field=\"Message\" mbr-theme-style=\"display-7\" value></textarea>\n                              </div>\n                              <div mbr-buttons=\"true\" mbr-theme-style=\"display-7\" data-toolbar=\"-mbrBtnMove,-mbrBtnLink,-mbrBtnAdd\" class=\"mbr-col-lg-12 mbr-section-btn field\">\n                                  <a type=\"submit\" class=\"btn btn-md btn-primary btn-form\">Send</a>\n                              </div>\n\n                          </div>\n                      </form>\n                    </div>\n                    <div class=\"mbr-col-md-12\">\n                        <p mbr-theme-style=\"display-7\" mbr-if=\"showFormText\" class=\"mbr-fonts-style mbr-fonts-style form-text mbr-black mbr-pt-2\">Write some\n                            info about your form for your site visitors here.</p>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
